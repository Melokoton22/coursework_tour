CREATE TABLE IF NOT EXISTS `bookings` (
	`id` int AUTO_INCREMENT NOT NULL UNIQUE,
	`client_id` int NOT NULL,
	`tour_id` int NOT NULL,
	`booking_date` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
	`status` ENUM('pending', 'approved', 'rejected') NOT NULL DEFAULT 'pending',
    `booking_price` decimal(10, 2) NOT NULL,
	`processed_by` int NOT NULL,
	PRIMARY KEY (`id`),
    FOREIGN KEY (`processed_by`) REFERENCES `employees`(`id`)
);

DELIMITER //

CREATE TRIGGER `set_booking_price`
BEFORE INSERT ON `bookings`
FOR EACH ROW
BEGIN
	DECLARE tourPrice DECIMAL(10, 2);

	-- Отримуємо вартість туру з таблиці tours
	SELECT `price` INTO tourPrice FROM `tours` WHERE `id` = NEW.tour_id;

	-- Встановлюємо значення booking_price
	SET NEW.booking_price = tourPrice;
END;

//

DELIMITER ;

INSERT INTO `bookings` (`id`, `client_id`, `tour_id`, `booking_date`, `status`, `booking_price`, `processed_by`)
