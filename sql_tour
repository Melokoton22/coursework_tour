ALTER TABLE `bookings` ADD CONSTRAINT `bookings_fk1` FOREIGN KEY (`client_id`) REFERENCES `clients`(`id`);

ALTER TABLE `bookings` ADD CONSTRAINT `bookings_fk2` FOREIGN KEY (`tour_id`) REFERENCES `tours`(`id`);
ALTER TABLE `payments` ADD CONSTRAINT `payments_fk1` FOREIGN KEY (`booking_id`) REFERENCES `bookings`(`id`);

ALTER TABLE `departments` ADD CONSTRAINT `departments_fk2` FOREIGN KEY (`manager_id`) REFERENCES `employees`(`id`);
